  <script>
  var selectCallbackQuickShop = function(variant, selector) {
    {% if template contains "pre-order" %}
      //var submitText = "{{ 'products.product.pre_order' | t }}";
    {% else %}
      //var submitText = "{{ 'products.product.add_to_cart' | t }}";
    {% endif %}
    var id = selector.domIdPrefix.split("-")[2];
    if (id) {
      context = $("#quick-shop-" + id)
    } else {
      context = $("#product-area, .mobile-product-title")
    }
    // this code would be for showing and hiding the out of stock messaging
    /*
    console.log('productJSON: ', productJSON);
    var available_sizes = [];
    for (var i=0; i < productJSON.variants.length; i++) {
      var this_variant = productJSON.variants[i]
      console.log('this_variant: ', this_variant);
    }
    console.log('variant: ', variant);
    var unavailable_sizes = [];
    $.each(productJSON.variants, function(i,variant){
      if(!variant.available){
          var checked_option = variant.option1;
          if(productJSON.options[1].toLowerCase()=='size'){
            checked_option = variant.option2;
          }
          unavailable_sizes.push(checked_option);
        }
    });
    // iterate over options and disable unavailable sizes
    $('.single-option-selector option').each(function(){
        if( inArray($(this).attr('value'), unavailable_sizes) ){
          //$(this).attr('disabled','disabled').append(' - Out of Stock');
          $(this).addClass('option-disabled').append(' - Out of Stock');
        }
    });
    */
    if (variant && variant.available == true ) {
      context.find(".submit").removeAttr("disabled");
      if ( variant.compare_at_price > 0 && variant.compare_at_price > variant.price ) {
        context.find('.price').html('<span class="original money" data-currency-' + Theme.currency + '="' + Shopify.formatMoney(variant.compare_at_price, Theme.moneyFormat) + '">' + Shopify.formatMoney(variant.compare_at_price, Theme.moneyFormat) + '</span> <span class="money" data-currency-' + Theme.currency + '="' + Shopify.formatMoney(variant.price, Theme.moneyFormat) + '">' + Shopify.formatMoney(variant.price, Theme.moneyFormat) + '</span>');  // update price field
      } else {
        context.find('.price').html('<span class="money" data-currency-' + Theme.currency + '="' + Shopify.formatMoney(variant.price, Theme.moneyFormat) + '">' + Shopify.formatMoney(variant.price, Theme.moneyFormat) + '</span>');  // update price field
      }
      var submit_label = context.find(".submit").data('default-label');
      var sold_out_label = context.find(".submit").data('sold-out-label');
      var default_label = context.find(".submit").data('default-label');
      if(variant.inventory_quantity <= 2) {
        $(".size-remaining").show().text("Only "+variant.inventory_quantity+" left in stock!")
      } else {
        $(".size-remaining").hide()
      }
      if(typeof submit_label != "undefined" &&  variant.inventory_quantity < 1){
        // Reserve Message with Sold Out Label
        if(typeof sold_out_label != "undefined" &&  sold_out_label ){
          context.find(".submit").removeClass("disabled").removeAttr("disabled").val(sold_out_label);
          context.find('.reserve-message').show();
          context.find('.friendly-message').hide();
        // Pre Order Message
        }else if(typeof default_label != "undefined" &&  default_label == 'Pre-order' ){
          context.find(".submit").removeClass("disabled").removeAttr("disabled").val(default_label);
          context.find('.reserve-message').show();
          context.find('.friendly-message').hide();
        }else{
          context.find(".submit").val( submit_label ); // remove unavailable class from add-to-cart button, and re-enable button
        }
      }else{
        context.find(".submit").val( "{{ 'products.product.add_to_cart' | t }}" ); // remove unavailable class from add-to-cart button, and re-enable button
      }
      context.find(".submit").removeClass("disabled"); // remove unavailable class from add-to-cart button, and re-enable button
    } else {
        var message = variant ? "{{ 'products.product.sold_out' | t }}" : "{{ 'products.product.unavailable' | t }}";
        context
          .find(".submit").addClass("disabled").attr("disabled", "disabled").val(message)
          .find(".price").text(message);
        context.find('.reserve-message').hide();
        context.find('.friendly-message').show();
        $(".size-remaining").hide()
    }
    if (variant && variant.featured_image) {
      var newImage = variant.featured_image;
      var smallImage = Shopify.Image.getSizedImageUrl(newImage.src, 'small');
      {% if template contains "product" %}
        $("#product-area .thumb img[src*='" + smallImage + "']").click();
      {% else %}
        $(".quick-shop-content .thumb img[src*='" + smallImage + "']").click();
      {% endif %}
    }
    if (Theme.currencySwitcher) {
      $(document.body).trigger('switch-currency');
    }
    // BEGIN SWATCHES
    if (variant) {
      var form = jQuery('#' + selector.domIdPrefix).closest('form');
      for (var i=0,length=variant.options.length; i<length; i++) {
        var radioButton = form.find('.swatch[data-option-index="' + i + '"] :radio[value="' + variant.options[i] +'"]');
        if (radioButton.size()) {
          radioButton.get(0).checked = true;
        }
      }
    }
    
  };
  </script>
