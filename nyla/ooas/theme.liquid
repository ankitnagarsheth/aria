  <script>
    // this handles the swatch clicking and selects the right variant in the hidden select
    jQuery(function() {
      jQuery('.swatch :radio').on('change',function() {
        var optionIndex = jQuery(this).closest('.swatch').attr('data-option-index');
        var optionValue = jQuery(this).val();
        var variant_image = jQuery(this).data('featured-image');
        if( variant_image ){
          var old_image = $('.product-main-image img').attr('src');
          var img_base = old_image.split('products')[0];
          var new_image = img_base + variant_image;
          $('.product-main-image img').attr('src', new_image);
        }
        jQuery(this)
          .closest('form')
          .find('.single-option-selector')
          .eq(optionIndex)
          .val(optionValue)
          .trigger('change');
        jQuery(this).closest('.swatch').find('span.swatch-selected-value').text(': ' + optionValue);
      });
    });
