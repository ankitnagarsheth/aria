          function fetchImage(url, i) {
            $.ajax({
              url: CORSProxy + url,
              type: "GET"
            }).done(function(html) {
              var image = $(html).find(".image-wrapper").first().html();
              image = $(image).attr("src").replace("7x5", "408x300");

              $("#articles li")
