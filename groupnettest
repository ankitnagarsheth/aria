        function trackNavigation(linkName, linkPosition) {
            var digitalData = digitalData || window.digitalData;
            digitalData.navInfo = null;

            if (digitalData) {
                var pageName = digitalData.page.pageInfo.pageName;
                digitalData.navInfo = {
                    linkPage: pageName,
                    linkName: linkName,
                    linkPosition: linkPosition
                };
                track('navClick');
            }
        };
        function track(key) {
            if (window._satellite) {
                window._satellite.track(key);
            }
        };
        _satellite.track("page");                
